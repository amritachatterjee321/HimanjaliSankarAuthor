{
  "rewrites": [
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/books",
      "destination": "/books.html"
    },
    {
      "source": "/media",
      "destination": "/media.html"
    },
    {
      "source": "/contact",
      "destination": "/contact.html"
    },
    {
      "source": "/cms",
      "destination": "/cms/index.html"
    },
    {
      "source": "/book/:id",
      "destination": "/book-detail.html"
    }
  ],
  "redirects": [
    {
      "source": "/about/",
      "destination": "/about",
      "permanent": true
    },
    {
      "source": "/books/",
      "destination": "/books",
      "permanent": true
    },
    {
      "source": "/media/",
      "destination": "/media",
      "permanent": true
    },
    {
      "source": "/contact/",
      "destination": "/contact",
      "permanent": true
    },
    {
      "source": "/cms/",
      "destination": "/cms",
      "permanent": true
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}
